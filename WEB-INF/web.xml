<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  version="2.5">

  <servlet>
    <servlet-name>SubmitSolutionServlet</servlet-name>
    <servlet-class>gdcontest.SubmitSolutionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SubmitSolutionServlet</servlet-name>
    <url-pattern>/SubmitSolutionServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SubmitProblemServlet</servlet-name>
    <servlet-class>gdcontest.SubmitProblemServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SubmitProblemServlet</servlet-name>
    <url-pattern>/SubmitProblemServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SubmitToolServlet</servlet-name>
    <servlet-class>gdcontest.SubmitToolServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SubmitToolServlet</servlet-name>
    <url-pattern>/SubmitToolServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SubmitCategoryServlet</servlet-name>
    <servlet-class>gdcontest.SubmitCategoryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SubmitCategoryServlet</servlet-name>
    <url-pattern>/SubmitCategoryServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>RenameCategoryServlet</servlet-name>
    <servlet-class>gdcontest.RenameCategoryServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RenameCategoryServlet</servlet-name>
    <url-pattern>/RenameCategoryServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SubmitTeamServlet</servlet-name>
    <servlet-class>gdcontest.SubmitTeamServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SubmitTeamServlet</servlet-name>
    <url-pattern>/SubmitTeamServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>InputFileServlet</servlet-name>
    <servlet-class>gdcontest.InputFileServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>InputFileServlet</servlet-name>
    <url-pattern>*.txt</url-pattern>
    <url-pattern>*.ipe</url-pattern>
  </servlet-mapping>
    
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

  <filter>
    <filter-name>ObjectifyFilter</filter-name>
    <filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>ObjectifyFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <listener>
    <listener-class>gdcontest.OfyHelper</listener-class>
  </listener>

</web-app>
